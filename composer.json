{
    "name": "roukmoute/doctrine-prefix-bundle",
    "description": "Listener that prefixes tables and sequences",
    "require": {
        "php": "^8.1",
        "doctrine/event-manager": "^2.0",
        "doctrine/orm": "^3.0",
        "symfony/config": "^6.4 || ^7.0",
        "symfony/dependency-injection": "^6.4 || ^7.0",
        "symfony/http-kernel": "^6.4 || ^7.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Mathias STRASSER",
            "email": "contact@roukmoute.fr"
        }
    ],
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.64",
        "phpstan/phpstan": "^2.0",
        "phpunit/phpunit": "^10.5 || ^11.0"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": { "Roukmoute\\DoctrinePrefixBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Roukmoute\\DoctrinePrefixBundle\\Tests\\": "tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1-dev"
        }
    }
}
